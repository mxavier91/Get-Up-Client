{{#each users as |user|}}

<h1 data-id="{{user.id}}">
  <small>
    {{user.username}}
  </small>
</h1>

{{#each workouts as |workout|}}

<table class="top">
  <thead>
    <th class="middle">ID</th>
    <th class="middle">Weight</th>
    <th class="middle">Exercise</th>
    <th class="middle">Reps</th>
    <th class="middle">Sets</th>
  </thead>
{{#if user.id}}
<tr>
  <td class="body">{{workout.id}}</td>
  <td class="body">{{workout.weight}}</td>
  <td class="body">{{workout.exercise}}</td>
  <td class="body">{{workout.reps}}</td>
  <td class="body">{{workout.sets}}</td>
</tr>
{{else}}
  Nothing provided
{{/if}}<br>
</table>

{{/each}}
{{/each}}
